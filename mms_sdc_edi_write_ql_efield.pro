	mods    = { 'v0.0.0 -- First version.', ...
                'v0.0.1 -- Added gun positions.', ...
                'v0.0.2 -- Include points with beams from single gun.' };

%------------------------------------------------------
% Global Attributes                                   |
%------------------------------------------------------
