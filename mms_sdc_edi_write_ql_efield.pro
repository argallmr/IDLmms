	                                       { tt2000_vname,      'I16',...
	                                         t_gd12_vname,      'I16',...
	                                         t_gd21_vname,      'I16',...
	                                         e_vname,           'F12.6', ...
	                                         v_vname,           'F12.6', ...
	                                         d_vname,           'F12.6', ...
	                                         b_vname,           'F12.6', ...
	                                         pos_vg1_vname,     'F12.6', ...
	                                         pos_vg2_vname,     'F12.6', ...
	                                         fv_gd12_vname,     'F12.6', ...
	                                         fv_gd21_vname,     'F12.6', ...
	                                         inds_gd12_vname,   'I9', ...
	                                         inds_gd21_vname,   'I9', ...
