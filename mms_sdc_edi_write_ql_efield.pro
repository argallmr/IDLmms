	e_vname           = mms_construct_varname(sc, instr, 'E', 'dmpa');
	v_vname           = mms_construct_varname(sc, instr, 'v', 'ExB_dmpa');
	d_vname           = mms_construct_varname(sc, instr, 'd', 'dmpa');
	b_vname           = mms_construct_varname(sc, instr, 'B', 'dmpa');
