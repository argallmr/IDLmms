	sc      = edi_ql.sc;
	instr   = edi_ql.instr;
	mode    = edi_ql.mode;
	level   = 'ql';
	optdesc = edi_ql.optdesc;

	% Create the output filename
	edi_file = mms_construct_filename(sc, instr, mode, level, ...
	                                  'Directory', edi_ql.save_dir, ...
	                                  'OptDesc',   optdesc, ...
	                                  'TStart',    tstart, ...
	                                  'Version',   'v0.0.2');
	                                  
