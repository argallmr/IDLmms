	                 t_gd12_vname,       'cdf_time_tt2000', ...
	                 t_gd21_vname,       'cdf_time_tt2000', ...
	                 e_vname,            'cdf_float', ...
	                 v_vname,            'cdf_float', ...
	                 d_vname,            'cdf_float', ...
	                 b_vname,            'cdf_float', ...
